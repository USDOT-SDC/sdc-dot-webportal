{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": "arn:aws:logs:*:*:*",
        "Effect": "Allow"
      },
      {
        "Action": [
          "ses:SendEmail",
          "pricing:DescribeServices",
          "pricing:GetAttributeValues",
          "s3:*",
          "apigateway:*",
          "appstream:*",
          "dynamodb:*",
          "ec2:*",
          "pricing:GetProducts"
        ],
        "Resource": "*",
        "Effect": "Allow"
      }
    ]
  }